<configuration xmlns:patch="http://www.sitecore.net/xmlconfig/">
    <sitecore>
        <services>
            <configurator type="Sitecore.Reference.Storefront.Infrastructure.ServicesConfigurator, Sitecore.Reference.Storefront"/>
        </services>

        <pipelines>
            <initialize>
                <!--<processor patch:before="processor[@type='Sitecore.Mvc.Pipelines.Loader.InitializeGlobalFilters, Sitecore.Mvc']" type="Sitecore.Reference.Storefront.Infrastructure.SitecorePipelines.InitializeBundles, Sitecore.Reference.Storefront" resolve="true"/>-->
                <processor patch:before="processor[@type='Sitecore.Mvc.Pipelines.Loader.InitializeGlobalFilters, Sitecore.Mvc']" type="Sitecore.Reference.Storefront.Infrastructure.SitecorePipelines.InitializeRoutes, Sitecore.Reference.Storefront" resolve="true"/>
            </initialize>
            <mvc.getPageRendering>
                <processor patch:after="*[@type='Sitecore.Foundation.Assets.Pipelines.GetPageRendering.AddAssets, Sitecore.Foundation.Assets']" type="Sitecore.Reference.Storefront.Infrastructure.SitecorePipelines.AddCommerceAssets, Sitecore.Reference.Storefront"/>
            </mvc.getPageRendering>
        </pipelines>

    </sitecore>
</configuration>