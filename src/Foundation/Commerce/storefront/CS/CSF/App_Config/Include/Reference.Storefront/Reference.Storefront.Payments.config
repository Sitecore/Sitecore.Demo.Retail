<configuration xmlns:patch="http://www.sitecore.net/xmlconfig/">
  <sitecore>
    <commerce.Entities>
    </commerce.Entities>

    <pipelines>
      <commerce.payments.getPaymentOptions>
        <processor type="Sitecore.Reference.Storefront.Connect.Pipelines.Payments.GetPaymentOptions, Sitecore.Reference.Storefront.Powered.by.CommerceServer">
          <param ref="entityFactory"/>
        </processor>
      </commerce.payments.getPaymentOptions>

      <commerce.payments.getPaymentMethods>
        <processor type="Sitecore.Commerce.Connect.CommerceServer.Orders.Pipelines.GetPaymentMethods, Sitecore.Commerce.Connect.CommerceServer">
          <patch:attribute name="type">Sitecore.Reference.Storefront.Connect.Pipelines.Payments.GetPaymentMethods, Sitecore.Reference.Storefront.Powered.by.CommerceServer</patch:attribute>
        </processor>
      </commerce.payments.getPaymentMethods>
    </pipelines>
  </sitecore>
</configuration>