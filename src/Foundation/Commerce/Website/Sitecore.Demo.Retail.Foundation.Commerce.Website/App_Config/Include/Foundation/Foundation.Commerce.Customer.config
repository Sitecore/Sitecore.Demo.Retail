<configuration xmlns:patch="http://www.sitecore.net/xmlconfig/">
    <sitecore>
        <pipelines>
            <commerce.customers.getUser>
                <processor type="Sitecore.Commerce.Pipelines.Customers.GetUser.GetUserFromSitecore, Sitecore.Commerce">
                    <patch:delete />
                </processor>
                <processor type="Sitecore.Demo.Retail.Foundation.Commerce.Website.Infrastructure.Connect.Pipelines.Customers.GetUser, Sitecore.Demo.Retail.Foundation.Commerce.Website">
                    <param ref="sitecoreUserRepository" />
                </processor>
            </commerce.customers.getUser>

            <commerce.customers.addParties>
                <processor type="Sitecore.Commerce.Pipelines.Customers.AddPartiesToExternalSystem, Sitecore.Commerce">
                    <patch:delete />
                </processor>
                <processor type="Sitecore.Commerce.Pipelines.Customers.AddPartiesToSitecore, Sitecore.Commerce">
                    <patch:delete />
                </processor>
                <processor type="Sitecore.Demo.Retail.Foundation.Commerce.Website.Infrastructure.Connect.Pipelines.Customers.AddParties, Sitecore.Demo.Retail.Foundation.Commerce.Website">
                    <param ref="entityFactory" />
                </processor>
            </commerce.customers.addParties>

            <commerce.customers.removeParties>
                <processor type="Sitecore.Commerce.Pipelines.Customers.RemovePartiesFromExternalSystem, Sitecore.Commerce">
                    <patch:delete />
                </processor>
                <processor type="Sitecore.Commerce.Pipelines.Customers.RemovePartiesFromSitecore, Sitecore.Commerce">
                    <patch:delete />
                </processor>
                <processor type="Sitecore.Demo.Retail.Foundation.Commerce.Website.Infrastructure.Connect.Pipelines.Customers.RemoveParties, Sitecore.Demo.Retail.Foundation.Commerce.Website"/>
            </commerce.customers.removeParties>

            <commerce.customers.updateCustomer>
                <processor type="Sitecore.Commerce.Pipelines.Customers.UpdateCustomer.UpdateCustomerInExternalSystem, Sitecore.Commerce">
                    <patch:attribute name="type">Sitecore.Demo.Retail.Foundation.Commerce.Website.Infrastructure.Connect.Pipelines.Customers.UpdateCustomer, Sitecore.Demo.Retail.Foundation.Commerce.Website</patch:attribute>
                </processor>
                <processor type="Sitecore.Commerce.Pipelines.Customers.UpdateCustomer.UpdateCustomerInExternalSystem, Sitecore.Commerce">
                    <patch:delete />
                </processor>
                <processor type="Sitecore.Demo.Retail.Foundation.Commerce.Website.Infrastructure.Connect.Pipelines.Customers.UpdateCustomer, Sitecore.Demo.Retail.Foundation.Commerce.Website"/>
            </commerce.customers.updateCustomer>

            <commerce.customers.updateParties>
                <processor type="Sitecore.Commerce.Pipelines.Customers.UpdatePartiesInExternalSystem, Sitecore.Commerce">
                    <patch:delete />
                </processor>
                <processor type="Sitecore.Commerce.Pipelines.Customers.UpdatePartiesInSitecore, Sitecore.Commerce">
                    <patch:delete />
                </processor>
                <processor type="Sitecore.Demo.Retail.Foundation.Commerce.Website.Infrastructure.Connect.Pipelines.Customers.UpdateParties, Sitecore.Demo.Retail.Foundation.Commerce.Website"/>
            </commerce.customers.updateParties>

            <commerce.customers.getParties>
                <processor type="Sitecore.Commerce.Pipelines.Customers.GetPartiesFromExternalSystem, Sitecore.Commerce">
                    <patch:delete />
                </processor>
                <processor type="Sitecore.Commerce.Pipelines.Customers.GetPartiesFromSitecore, Sitecore.Commerce">
                    <patch:delete />
                </processor>
                <processor type="Sitecore.Demo.Retail.Foundation.Commerce.Website.Infrastructure.Connect.Pipelines.Customers.GetParties, Sitecore.Demo.Retail.Foundation.Commerce.Website">
                    <param ref="entityFactory" />
                </processor>
            </commerce.customers.getParties>

            <translate.entityToCommerceAddressProfile>
                <processor type="Sitecore.Demo.Retail.Foundation.Commerce.Website.Infrastructure.Connect.Pipelines.Customers.TranslateEntityToCommerceAddressProfile, Sitecore.Demo.Retail.Foundation.Commerce.Website"/>
            </translate.entityToCommerceAddressProfile>

            <translate.commerceAddressProfileToEntity>
                <processor type="Sitecore.Demo.Retail.Foundation.Commerce.Website.Infrastructure.Connect.Pipelines.Customers.TranslateCommerceAddressProfileToEntity, Sitecore.Demo.Retail.Foundation.Commerce.Website"/>
            </translate.commerceAddressProfileToEntity>

        </pipelines>
    </sitecore>
</configuration>