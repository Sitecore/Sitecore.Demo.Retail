<?xml version="1.0"?>

<configuration xmlns:patch="http://www.sitecore.net/xmlconfig/">
  <sitecore>
      <services>
          <configurator type="Sitecore.Feature.Commerce.Orders.Website.Infrastructure.ServicesConfigurator, Sitecore.Feature.Commerce.Orders.Website"/>
      </services>

      <pipelines>
          <initialize>
              <processor type="Sitecore.Feature.Commerce.Orders.Website.Infrastructure.Pipelines.InitializeRoutes, Sitecore.Feature.Commerce.Orders.Website" resolve="true" patch:before="processor[@type='Sitecore.Mvc.Pipelines.Loader.InitializeGlobalFilters, Sitecore.Mvc']" />
          </initialize>
      </pipelines>

      <commerce.Entities>
          <CartLine type="Sitecore.Commerce.Connect.CommerceServer.Orders.Models.CommerceCartLine, Sitecore.Commerce.Connect.CommerceServer">
              <patch:attribute name="type">Sitecore.Feature.Commerce.Orders.Website.Models.CommerceCartLineWithImages, Sitecore.Feature.Commerce.Orders.Website</patch:attribute>
          </CartLine>
      </commerce.Entities>
      
  </sitecore>
</configuration>