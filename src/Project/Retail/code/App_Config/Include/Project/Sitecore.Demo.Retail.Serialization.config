          <userDataStore type="Unicorn.Users.Data.FilesystemUserDataStore, Unicorn.Users" physicalRootPath="$(sitecore.demo.retail.sourceFolder)\project\retail\serialization\Sitecore.Demo.Retail.Users" singleInstance="true"/>
